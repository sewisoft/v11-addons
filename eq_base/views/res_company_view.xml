                    <label string="CEO"/>
                    <div>
                        <field name="eq_ceo_title" style="width: 100%%;" placeholder="Title" />
                        <field name="eq_ceo_01" style="width: 100%%;" />
                        <field name="eq_ceo_02" style="width: 100%%;" />
                        <field name="eq_ceo_03" style="width: 100%%;" />
                    </div>
                </field>
            </field>
        </record>
    </data>
</odoo>