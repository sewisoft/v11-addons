        <delete model="ir.translation" search="[('res_id','=', ref('sale_stock.field_sale_order_incoterm'))]"/>

        <record id="eq_sale_stock_incoterm_translation" model="ir.translation">
            <field name="name">ir.model.fields,field_description</field>
            <field name="type">model</field>
            <field name="res_id" ref="sale_stock.field_sale_order_incoterm"/>
