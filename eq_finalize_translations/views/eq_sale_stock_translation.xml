            <field name="module">sale_stock</field>
            <field name="state">translated</field>
            <field name="lang">de_DE</field> <!-- The code of the language you want to translate to -->
            <field name="src">Incoterms</field>
            <field name="value">Incoterms</field> <!-- vorher fehlerhafte Übersetzung 'Mininale Menge' -->
