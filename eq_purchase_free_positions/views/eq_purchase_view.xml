<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="eq_view_purchase_config_settings_report_settings1" model="ir.ui.view">
            <field name="name">eq_purchase.config.report.settings1</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="purchase.res_config_settings_view_form_purchase"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@data-key='purchase']" position="inside">
                    <h2>Free position</h2>
                    <div class="row mt16 o_settings_container" id="eq_purchase_report_settings">
                        <div class="col-xs-12 col-md-6 o_setting_box" id="eq_free_position">
                            <div class="o_setting_right_pane">
                                <label string="Tax"/>
                                <div class="content-group">
                                    <div class="mt16">
                                        <field name="eq_default_tax_purchase_order" />
                                        <label for="eq_default_tax_purchase_order" class="o_light_label oe_inline"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>